{
    "name": "@xwink/utils",
    "version": "1.0.0",
    "keywords": [
        "typescript",
        "libary",
        "utils"
    ],
    "author": {
        "name": "XWINK",
        "email": "1041367524@qq.com",
        "url": "https://github.com/x-wink"
    },
    "repository": {
        "url": "https://github.com/x-wink/wink-utils"
    },
    "private": false,
    "files": [
        "dist",
        "types"
    ],
    "type": "module",
    "main": "dist/index.mjs",
    "module": "dist/index.mjs",
    "types": "types/index.d.ts",
    "browser": "dist/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "dev": "vite docs",
        "build": "tsc && vite build",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag && pnpm run build && pnpm publish --access public",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "prettier": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,json,md,html}",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.1",
        "@commitlint/config-conventional": "^18.4.0",
        "@vitejs/plugin-vue": "^4.4.1",
        "@xwink/eslint-config": "1.1.1-beta.1",
        "bumpp": "^9.2.0",
        "conventional-changelog-cli": "^4.1.0",
        "eslint": "^8.53.0",
        "husky": "^8.0.3",
        "less": "^4.2.0",
        "lint-staged": "^15.1.0",
        "prettier": "^3.1.0",
        "rollup-plugin-visualizer": "^5.9.2",
        "typescript": "~5.2.2",
        "vite": "^4.5.0",
        "vue": "^3.3.8",
        "vue-tsc": "^1.8.22"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": [
            "npm run lint"
        ],
        "*.{vue,ts,tsx,js,jsx,css,less,json,md,html}": [
            "npm run prettier"
        ]
    }
}
